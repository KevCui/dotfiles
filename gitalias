[alias]
    a = add
    s = status
    ci = commit -m
    co = checkout
    p = push
    pll = pull
	d = difftool
    b = branch
    m = merge
    l = log
    i = init
    undo = checkout --
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
